<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:VM="clr-namespace:ParaisoRealA.ViewModel"
             x:Class="ParaisoRealA.View.Login">

    <ContentPage.BindingContext>
        <VM:LoginVM></VM:LoginVM>
    </ContentPage.BindingContext>
   
    <ContentPage.Content>
        <StackLayout>
            <Image Source="Logo.png"></Image>
            <Entry Placeholder="Usuario"></Entry>
            <Entry Placeholder="Contraseña"></Entry>
            
        </StackLayout>
    </ContentPage.Content>
</ContentPage>