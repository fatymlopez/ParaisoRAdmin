<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="ParaisoRealA.View.VerDetalleU">

   
        <StackLayout>
            <Label Text="Nombre"></Label>
            <Label Text="{Binding NombreU}"></Label>

            <Label Text="Apellido"></Label>
            <Label Text="{Binding ApellidoU}"></Label>

            <Label Text="Usuario"></Label>
            <Label Text="{Binding MostrarU}"></Label>

            <Label Text="Contraseña"></Label>
            <Label Text="{Binding MostrarC}"></Label>

            <Button Text="Editar" Command="{Binding EditarPCommand}"></Button>

        </StackLayout>
</ContentPage>